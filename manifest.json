{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "description": "yskproxy, manage pac easy and comfortable",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   }, 
   "manifest_version": 2,
   "name": "yskproxy",
   "permissions": [
       "tabs",
       "proxy", 
       "storage",  
       "declarativeContent",
       "*://*/*"
   ],
   "version": "1",
   "browser_specific_settings": {
      "gecko": {
        "id": "yskproxy-firefox-extension@netroby.com",
        "strict_min_version": "56.0"
      }
    }
}
